<%= hidden_field_tag "date[year]" %>
<%= hidden_field_tag "date[month]" %>
<%= hidden_field_tag "date[day]" %>
<% 12.times do |n| %>
  <div class="text-center">
    <% params[:start_date]=Date.today + n.month %>
    <%= month_calendar do |date| %>
      <%= date.day %>
    <% end %>
  </div>
<% end %>