  <% @spots.each do |spot|%>
  <div class="modal-content-card">
    <div class=spot-image>
      <%= image_tag("#{spot.images.first.image}") %>
    </div>
    <div class="modal-content-card-details">
      <p><%= spot.name %></p>
      <button class="modal-add-button"><i class="fas fa-plus-circle"></i></button>
    </div>
  </div>
<% end %>